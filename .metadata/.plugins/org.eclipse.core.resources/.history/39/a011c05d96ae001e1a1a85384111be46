package Data;

import Util.Cw;

public class CharPoint {
	int charTop = 0;
	int charBottom = 0;
	int charLeft = 0;
	int charRight = 0;
	
	
	public CharPoint(int t,int b,int l,int r){
		this.charTop = t;
		this.charBottom = b;
		this.charLeft = l;
		this.charRight = r;
	}
	
	public void charPointInfo() {
		Cw.wn("현재 캐릭터 좌표값 :[" + this.charTop+","+this.charBottom+","+this.charLeft+","+this.charRight+"]" );
	}

}
