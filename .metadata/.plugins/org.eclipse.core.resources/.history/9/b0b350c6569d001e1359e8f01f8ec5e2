package com.peisia.util.javamemory;

public class Main {
	public static void main(String[] args) {
		Cat kitty = new Cat();
		kitty.age = 3;
		
		System.out.println(kitty.age);
		
		Cat yaongi;
		
		yaongi = kitty;
		
		yaongi.age = 4;
		
		
		System.out.println(kitty.age);
		System.out.println(yaongi.age);
		
		kitty.age = 10;
		
		System.out.println(kitty.age);
		System.out.println(yaongi.age);
	}
}
